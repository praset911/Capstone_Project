{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/services/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/" },
    { "source": "/articles", "destination": "/" },
    { "source": "/calc-it", "destination": "/" },
    { "source": "/about-us", "destination": "/" }
  ]
}
